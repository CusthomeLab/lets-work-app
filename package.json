{
  "name": "lets-work-app",
  "productName": "Lets Work",
  "version": "1.1.1",
  "description": "App to easily open work tools and make your life easier !",
  "private": true,
  "author": "Magelan",
  "repository": {
    "type": "git",
    "url": "https://github.com/magelanLab/lets-work-app.git"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@atlaskit/logo": "^9.2.3",
    "antd": "^3.10.3",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^3.2.3",
    "node-osascript": "^2.1.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.0.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --x64 --macos",
    "ship": "electron-builder --x64 --macos --win -p always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-import": "^1.11.0",
    "concurrently": "^4.0.1",
    "electron": "^3.0.6",
    "electron-builder": "^20.28.4",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "2.0.5",
    "wait-on": "^3.1.0"
  }
}
